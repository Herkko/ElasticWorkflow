            var RaphaelElement;
	    window.onload =init();
		
            function init() {
            	RaphaelElement = Raphael("holder", 600, 500);
            		
            	var startCircle = RaphaelElement.circle(80,70,40).attr({
            	fill: "white"
            	});
			
		var ajaxURL = "http://localhost:9000/show";	
		$.getJSON(ajaxURL, startCircle, function(){
			$.('#p').val("onnistui");	
			};
